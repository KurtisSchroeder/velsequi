{
  "name": "velsequi",
  "version": "1.0.0",
  "description": "Swarm JavaScript API",
  "main": "dist/buzz.js",
  "module": "dist/buzz.mjs",
  "jsnext:main": "dist/buzz.mjs",
  "directories": {
    "doc": "doc",
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KurtisSchroeder/velsequi.git"
  },
  "keywords": [
    "accusantium",
    "bzz",
    "qui",
    "doloribus",
    "in"
  ],
  "author": "ConorFay",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix \"lib/**/*.js\"",
      "prettier --write --no-semi --single-quote --trailing-comma=es5",
      "git add"
    ]
  },
  "pre-commit": {
    "run": [
      "lint:staged",
      "test"
    ],
    "silent": true
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.0",
    "np": "^2.20.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-butternut": "^0.1.0"
  },
  "dependencies": {
    "buffer": "^5.1.0",
    "node-fetch": "^2.1.2"
  }
}